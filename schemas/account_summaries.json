{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "kind",
    "id",
    "name",
    "status",
    "plan"
  ],
  "properties": {
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "pattern": "^(.*)$"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "plan": {
      "$id": "#/properties/plan",
      "type": "string",
      "title": "The Plan Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "days_left": {
      "$id": "#/properties/days_left",
      "type": "integer",
      "title": "The Days_left Schema",
      "default": 0
    }
  },
  "additionalProperties": false
}
