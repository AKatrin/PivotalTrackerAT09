{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "kind",
      "id",
      "name",
      "person_id",
      "project_ids"
    ],
    "properties": {
        "kind": {
            "$id": "#/properties/kind",
            "type": "string",
            "title": "The Kind Schema",
            "pattern": "^(.*)$"
        },
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The Id Schema"
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "pattern": "^(.*)$"
        },
        "person_id": {
            "$id": "#/properties/person_id",
            "type": "integer",
            "title": "The Person_id Schema"
        },
        "project_ids": {
            "$id": "#/properties/project_ids",
            "type": "array",
            "title": "The Project_ids Schema",
            "items": {
                "$id": "#/properties/project_ids/items",
                "type": "integer",
                "title": "The Items Schema"
            }
        }
    },
    "additionalProperties": false
}